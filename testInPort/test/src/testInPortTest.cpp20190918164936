extern "C"
{
 
  void testInPortTestInit(RTC::Manager* manager)
  {
    coil::Properties profile(testinport_spec);
    manager->registerFactory(profile,
                             RTC::Create<testInPortTest>,
                             RTC::Delete<testInPortTest>);
  }
  
};


